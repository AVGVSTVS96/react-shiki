{
  "name": "react-shiki",
  "private": true,
  "packageManager": "pnpm@9.6.0",
  "version": "0.0.1",
  "description": "syntax highlighting component for react using shiki",
  "main": "index.js",
  "scripts": {
    "dev": "pnpm --stream -r --parallel dev",
    "package:dev": "pnpm --filter react-shiki dev",
    "package:build": "pnpm --filter react-shiki build",
    "package:publish": "pnpm --filter react-shiki publish",
    "lint": "biome check .",
    "lint:fix": "biome check --apply ."
  },
  "keywords": [
    "react",
    "shiki",
    "syntax",
    "highlighting"
  ],
  "author": "Bassim Shahidy",
  "license": "MIT",
  "dependencies": {
    "shiki": "^1.12.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "react": "^18.3.1",
    "typescript": "^5.5.4"
  },
  "peerDependencies": {
    "react": ">= 15.0.0"
  }
}
